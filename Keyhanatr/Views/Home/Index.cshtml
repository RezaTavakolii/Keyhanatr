@section slider
{
    <cache expires-after="@TimeSpan.FromMinutes(360)">
        @await Component.InvokeAsync("Slider")
    </cache>
}

@section products
{
    @await Component.InvokeAsync("Product")
}

@section banners
{
    <partial name="_Banner" />
}

